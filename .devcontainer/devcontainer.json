{
  "name": "devcontainer",
  "image": "registry.cn-shanghai.aliyuncs.com/amt/dev-node-java:20240315",
  "runArgs": [
    "--cap-add=NET_ADMIN",
    "--device=/dev/net/tun"
  ],
  "containerEnv": {
    "TZ": "Asia/Shanghai"
  },
  "mounts": [
    {
      "source": "/root/ctroot",
      "target": "/root",
      "type": "bind"
    },
    {
      "source": "/workspaces",
      "target": "/workspaces",
      "type": "bind"
    }
  ],
  "workspaceFolder": "/workspaces/.starter"
}